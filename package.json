{
  "name": "curriculum-vitae",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "build:clean": "yarn clean && NODE_ENV=production webpack --config webpack.prod.js",
    "clean": "rm -rf ./dist",
    "test": "yarn test:lintsass && yarn test:lintjs && yarn test:unit",
    "test:lintjs": "eslint ./ --ext 'js,jsx'",
    "test:lintsass": "sass-lint -v",
    "test:unit": "jest --coverage",
    "prettier": "prettier"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.4.5",
    "aws-sdk": "^2.494.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.4.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-dotenv-import": "^2.0.0",
    "base-watch": "^0.1.3",
    "concurrently": "^4.1.1",
    "css-loader": "^2.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.5.1",
    "file-loader": "^3.0.1",
    "global": "^4.3.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^23.6.0",
    "jest-cli": "^24.4.0",
    "lint-staged": "^8.1.7",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.6.0",
    "nodemon": "^1.17.2",
    "npm-watch": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.1",
    "prop-types": "15.6.1",
    "query-string": "5.1.1",
    "sass": "^1.0.0-beta.5.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "simple-get": "^3.0.2",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "autoprefixer": "^9.4.3",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "dotenv": "^6.2.0",
    "dotenv-webpack": "^1.5.7",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-inlinesvg": "^0.8.4",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.16.2"
  },
  "sasslintConfig": ".sass-lint.yml",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint ./ --fix",
      "git add",
      "jest --bail --findRelatedTests"
    ]
  },
  "engines": {
    "node": "10.16.3"
  },
  "engineStrict": true,
  "browserslist": [
    "> 1% in GB",
    "last 2 versions",
    "Firefox ESR",
    "iOS >= 8",
    "not ie < 11"
  ]
}
